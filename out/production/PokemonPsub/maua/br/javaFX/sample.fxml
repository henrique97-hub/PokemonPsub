<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="maua.br.Controller">
   <children>
      <VBox alignment="CENTER" layoutX="158.0" prefHeight="464.0" prefWidth="270.0" spacing="8.0">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <children>
            <Label contentDisplay="BOTTOM" text="Pokemons" />
            <TextField fx:id="txtUrl" promptText="URL Foto:" />
            <TextField fx:id="txtId" promptText="ID:" />
            <TextField fx:id="txtNome" promptText="Nome:" />
            <TextField fx:id="txtRaridade" promptText="Raridade" />
            <TextField fx:id="txtSerie" promptText="Serie:" />
            <TextField fx:id="txtColecao" promptText="Colocao:" />
            <ImageView fx:id="imgFoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Button mnemonicParsing="false" onAction="#previewPhoto" text="Ver Foto" />
            <Button mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar" />
            <Button mnemonicParsing="false" text="Alterar" />
         </children>
      </VBox>
   </children>
</AnchorPane>
